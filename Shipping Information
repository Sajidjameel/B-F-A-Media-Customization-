                    const listItems = document.querySelectorAll('.navPages-item');

                    listItems.forEach((li) => {
                        // Get the category name from the nav link's text content
                        const category = li.textContent.trim();

                        // Check if there's corresponding content in the divContents object
                        if (divContents[category]) {
                            const newDiv = document.createElement('div');
                            newDiv.innerHTML = divContents[category];
                            li.appendChild(newDiv);

                            // Set up hover events to show/hide the div on mouseenter and mouseleave
                            li.addEventListener('mouseenter', function () {
                                if (window.innerWidth >= 768) {
                                    const contentDiv = this.querySelector('.main-container1');
                                    if (contentDiv) contentDiv.style.display = 'flex';
                                }
                            });

                            li.addEventListener('mouseleave', function () {
                                if (window.innerWidth >= 768) {
                                    const contentDiv = this.querySelector('.main-container1');
                                    if (contentDiv) contentDiv.style.display = 'none';
                                }
                            });
                        }
                    });
                });
