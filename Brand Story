                <script>
                    const dropdown = document.querySelector('#cart-preview-dropdown')

                    function windowSideClick() {
                        function handleSideClick(e) {
                            console.log(e, 'event')
                            dropdown.classList.remove('d-block')
                            window.removeEventListener('click', handleSideClick)
                        }
                        window.addEventListener('click', handleSideClick)
                    }
                    document.addEventListener('DOMContentLoaded', () => {
                        const cart = document.querySelector('.navUser-item--cart')
                        cart.addEventListener('click', () => {
                            dropdown.classList.add('d-block')
                            window.setTimeout(() => windowSideClick(), 1)
                        })
                    })
                </script>
