                        <script>
                            function toggleAnswer(element) {
                                const answer = element.nextElementSibling;
                                const plusIcon = element.querySelector(".plus");

                                if (answer.style.display === "block") {
                                    answer.style.display = "none";
                                    plusIcon.innerHTML = "+";
                                } else {
                                    answer.style.display = "block";
                                    plusIcon.innerHTML = "-";
                                }
                            }

                        </script>
