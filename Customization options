    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const mobileNavItems = document.querySelectorAll(".navPages-item");

            const dropdownData = {
                collections: [
                    { text: "COASTAL", url: "/coastal/" },
                    { text: "MODERN", url: "/modern/" },
                    { text: "GEOMETRIC", url: "/geometric/" },
                    { text: "FAVORITE", url: "/bedroom-favorite/" },
                ],
                pillows: [
                    { text: "THROW PILLOWS", url: "/pillows/sphere/" },
                    { text: "BOLSTER PILLOWS", url: "/pillows/bolster/" },
                    { text: "GEOMETRIC PILLOWS", url: "/geometric-pillows/" },
                ],
                cushions: [{ text: "CUSHION", url: "/cushions/" }],
                fabric_trim: [
                    { text: "FABRIC BY THE YARD", url: "/fabric-by-yard/" },
                    { text: "TRIM BY THE YARD", url: "/trim-by-yard/" },
                ],
                resources: [
                    { text: "DESIGN RESOURCES", url: "/design-resources/" },
                    { text: "FREE DESIGN RESOURCES", url: "/free-design-resources/" },
                    { text: "REVIEWS", url: "/reviews/" },
                    { text: "TRADE SUPPORT", url: "/trade-support/" },
                    { text: "FAQ", url: "/faq/" },
                ],
            };

            const menuMapping = {
                "Collections": "collections",
                "Pillows": "pillows",
                "Cushions": "cushions",
                "FABRIC &TRIM": "fabric_trim",
                "RESOURCES": "resources"
            };

            const createDropdownList = (items) => {
                const dropdownList = document.createElement("ul");
                dropdownList.classList.add("custom-dropdown-list");

                items.forEach(({ text, url }) => {
                    const listItem = document.createElement("li");
                    listItem.classList.add("custom-dropdown-item");

                    const anchor = document.createElement("a");
                    anchor.href = url;
                    anchor.textContent = text;
                    anchor.classList.add("custom-dropdown-link");

                    listItem.appendChild(anchor);
                    dropdownList.appendChild(listItem);
                });

                return dropdownList;
            };

            mobileNavItems.forEach((item) => {
                const actionLink = item.querySelector(".navPages-action");
                const menuText = actionLink.textContent.trim();
                const normalizedKey = menuMapping[menuText];
                const dropdownItems = dropdownData[normalizedKey];

                if (dropdownItems) {

                    const dropdownList = createDropdownList(dropdownItems);
                    item.appendChild(dropdownList);


                    const dropdownIcon = document.createElement("span");
                    dropdownIcon.classList.add("dropdown-icon");
                    dropdownIcon.innerHTML = "&#9662;";
                    actionLink.appendChild(dropdownIcon);


                    actionLink.addEventListener("click", function (event) {
                        event.preventDefault();


                        mobileNavItems.forEach(navItem => {
                            if (navItem !== item) navItem.classList.remove("custom-is-open");
                        });


                        item.classList.toggle("custom-is-open");
                    });
                }
            });


            window.addEventListener("resize", function () {
                if (window.innerWidth > 768) {
                    mobileNavItems.forEach(item => item.classList.remove("custom-is-open"));
                }
            });
        });


        const buttons = document.querySelectorAll('.button-13');
        buttons.forEach(button => {
            button.addEventListener('click', () => {
                window.location.href = 'https://centrocushions.com/?masterSection=Shape&subSection=THROW_PILLOW&cameraPosition=-0.732%2C0.429%2C0.520&cameraTarget=0.000%2C0.000%2C0.000&cameraZoom=1';
            });
        });


    </script>

